{
    "title": "Neal's NoteBook",
    "description": "一只程序猿的总结及学习内容。",
    "author": "Neal Zhao <neal73.zhao@outlook.com>",
    "output.name": "site",
    "language": "zh-hans",
    "gitbook": "3.2.3",
    "root": ".",
    "structure": {
        "readme": "README.md"
    },
    "links": {
        "sidebar": {
            "Neal's Blog": "http://neal73.gitbook.io/space"
        }
    },
    "plugins": [
        "-lunr",
        "-search",
        "search-plus@^0.0.11",
        "simple-page-toc@^0.1.1",
        "disqus@^0.1.0",
        "github@^2.0.0",
        "github-buttons@2.1.0",
        "splitter@^0.0.8",
        "expandable-chapters-small@^0.1.7",
        "katex@^1.1.3",
        "graph@^0.1.0",
        "mermaid-gb3@2.1.0",
        "tbfed-pagefooter@^0.0.1",
        "versions-select@^0.1.1"
    ],
    
    "pluginsConfig": {
        "disqus": {
            "shortName": "gitbookuse"
        },
        "github": {
            "url": "https://github.com/CGZ0703/neal-Note"
        },
        "github-buttons": {
            "repo": "CGZ0703/neal-Note",
            "types": [
                "star"
            ],
            "size": "small"
        },
        "tbfed-pagefooter": {
            "copyright":"Copyright &copy neal_zhao.com 2019",
            "modify_label": "该文件修订时间：",
            "modify_format": "YYYY-MM-DD HH:mm:ss"
        },
        "simple-page-toc": {
            "maxDepth": 3,
            "skipFirstH1": true
        },
        "versions": {
            "options": [
                {
                    "value": "http://neal73.gitbook.io/space",
                    "text": "v3.2.3"
                }
            ]
        }
    }
}
